{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icons/19.png",
         "38": "icons/38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Run Cake"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "ja",
   "description": "Happy Cake",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "19": "icons/19.png",
      "32": "icons/32.png",
      "38": "icons/38.png",
      "48": "icons/48.png"
   },
   "manifest_version": 2,
   "name": "Cake",
   "options_page": "options.html",
   "permissions": [
      "activeTab",
      "storage"
   ],
   "version": "0.1.1"
}
